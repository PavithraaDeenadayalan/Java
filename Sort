public class Main
{
	public static void main(String[] args) {
	    
	    int[] n = {4,2,3};
	    int k = 1, temp = 0;
	    
	    for(int i = 0; i < n.length - 1; i++){
	        if( n[i] > n[i+1]){
	            temp = n[i];
	            n[i] = n[i+1];
	            n[i+1] = temp;
	        }
	    }
	    for (int i = 0; i < n.length ; i++){
	        System.out.println(n[i]);
	    }
	}
}
